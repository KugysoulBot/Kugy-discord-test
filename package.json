{
  "name": "kugy-bot",
  "version": "1.1.0",
  "description": "Discord bot with music functionality using FFmpeg v7 compatible configuration",
  "main": "index-ffmpeg-v7-compatible.mjs",
  "type": "module",
  "scripts": {
    "start": "node index-ffmpeg-v7-compatible.mjs",
    "start:legacy": "node Index.mjs",
    "start:local": "node index-without-lavalink.mjs",
    "dev": "node --watch index-ffmpeg-v7-compatible.mjs",
    "dev:legacy": "node --watch Index.mjs",
    "dev:local": "node --watch index-without-lavalink.mjs",
    "clean": "./cleanup-and-restart.sh",
    "ffmpeg:check": "ffmpeg -version"
  },
  "dependencies": {
    "discord-player": "^7.1.0",
    "discord-player-youtubei": "^1.5.0",
    "discord.js": "^14.21.0",
    "dotenv": "^16.6.1",
    "mongoose": "^8.16.4",
    "ffmpeg-static": "^5.2.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "optionalDependencies": {
    "@discord-player/extractor": "^7.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "ffmpeg": ">=7.0.0"
  },
  "keywords": [
    "discord",
    "bot",
    "music",
    "ffmpeg-v7",
    "youtube",
    "nodejs"
  ],
  "author": "KugySoul",
  "license": "MIT"
}
